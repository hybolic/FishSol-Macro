{
    "recurseDepth": 10,
    "plugins": ["jsDocPlugin.js","plugins/underscore.js"],
    "source": {
        "includePattern": ".+\\.ahk?$",
        "include": ["./core", "./plugins", "./"],
        "excludePattern": "(.+1\\.9\\.[234])|(.+\\-v0\\.[0-9])|(Example)|Copy|docs|JSON.ahk|Roblo__xWindow.ahk|CapturePoints.ahk"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default":{
            "outputSourceFiles":false,
            "includeDate": false
        }
    },
    "opts": {
        "destination": ".\\docs\\",
        "readme": "README.md",
        "verbose": true,
        "debug": true
    }
}